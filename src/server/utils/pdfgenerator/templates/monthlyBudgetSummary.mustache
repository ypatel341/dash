        <div class="bucket-section">
          <div class="bucket-header">
            <h3>Groceries - Used: ${{monthlyExpenseTotal}} | Allocated: ${{monthlyBucketAllocation}}</h3>
          </div>
          {{#monthlyExpenses.length}}
            <table border="1" cellspacing="0" cellpadding="5">
              <tr>
                <th>Person</th>
                <th>Amount</th>
                <th>Vendor</th>
                <th>Description</th>
                <th>Expense Date</th>
              </tr>
              {{#monthlyExpenses}}
                <tr>
                  <td>{{person}}</td>
                  <td>${{amount}}</td>
                  <td>{{vendor}}</td>
                  <td>{{description}}</td>
                  <td>{{expensedate}}</td>
                </tr>
              {{/monthlyExpenses}}
            </table>
          {{/monthlyExpenses.length}}
          {{^monthlyExpenses.length}}
            <p class="no-expenses"><em>No expenses recorded for this bucket this month</em></p>
