<div class="page-content">
  <div class="yearly-summary section-break">
    <h2>Yearly Accumulated Summary Since Start of Year</h2>
    <table border="1" cellspacing="0" cellpadding="5">
      <tr>
        <th>Bucket Name</th>
        <th>Yearly Spent</th>
        <th>Yearly Allocated</th>
      </tr>
      {{#yearlyAccumulatedData}}
        <tr>
          <td>{{bucketname}}</td>
          <td>${{yearlyAccumulated}}</td>
          <td>${{yearlyAllocated}}</td>
        </tr>
      {{/yearlyAccumulatedData}}
    </table>
  </div>

