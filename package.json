{
  "name": "dash",
  "version": "0.1.0",
  "engines": {
    "node": "20.x"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.1",
    "@mui/x-date-pickers": "^7.24.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^16.18.79",
    "axios": "^1.7.9",
    "compression": "^1.8.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "pg": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.3",
    "react-scripts": "^5.0.1",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.4",
    "webpack": "^5.98.0",
    "winston": "^3.17.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build-frontend": "react-scripts build",
    "build-backend": "tsc",
    "start": "react-scripts start",
    "start-backend": "NODE_ENV=development node dist/server/index.js",
    "start-all": "NODE_ENV=development concurrently \"npm run start\" \"npm run start-backend\"",
    "start-frontend-prod": "npx serve -s build -l 3000",
    "start-backend-prod": "NODE_ENV=production node dist/server/index.js",
    "start-all-prod": "concurrently \"npm run start-frontend-prod\" \"npm run start-backend-prod\"",
    "prettier": "prettier --write .",
    "test": "NODE_ENV=development jest --detectOpenHandles --verbose",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cleanup": "ts-prune && cspell '**/*.{js,jsx,ts,tsx,md,json}'",
    "test:int": "start-server-and-test start-all http://localhost:3000 cy:open",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run test",
      "npm run cy:run",
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/cypress": "^1.1.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "babel-loader": "^9.2.1",
    "cspell": "^8.17.3",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.1",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.10",
    "style-loader": "^4.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2"
  }
}
