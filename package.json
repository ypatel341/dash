  "scripts": {
    "build": "tsc",
    "start": "react-scripts start",
    "start-backend": "node dist/server/index.js",
    "start-all": "concurrently \"npm run start\" \"npm run start-backend\"",
    "prettier": "prettier --write .",
    "test": "jest",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:int": "start-server-and-test start-all http://localhost:3000 cy:open",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run test",
      "npm run cy:run",
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@commitlint/cli": "^19.6.1",
